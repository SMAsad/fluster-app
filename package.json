        "FCM_VERSION": "11.8.0"
      },
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-x-socialsharing": {
        "ANDROID_SUPPORT_V4_VERSION": "24.1.1+"
      },
      "cordova-plugin-googleplus": {
        "REVERSED_CLIENT_ID": "{{GOOGLE_LOGIN_REVERSED_CLIENT_ID}}",
        "PLAY_SERVICES_VERSION": "11.8.0"
      },
      "cordova-plugin-calendar": {
        "CALENDAR_USAGE_DESCRIPTION": "If you wish to export your viewings to your calendar, Fluster requires access to it",
        "CONTACTS_USAGE_DESCRIPTION": "If you wish to use your contacts, Fluster requires access to it"
      },
      "branch-cordova-sdk": {},
      "cordova-plugin-facebook4": {
        "APP_ID": "{{FACEBOOK_APP_ID}}",
        "APP_NAME": "Fluster",
        "FACEBOOK_HYBRID_APP_EVENTS": "false",
        "FACEBOOK_ANDROID_SDK_VERSION": "4.40.0"
      },
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova.plugins.diagnostic": {
        "ANDROID_SUPPORT_VERSION": "28.+"
      }
    }
