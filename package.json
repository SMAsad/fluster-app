  "cordova": {
    "platforms": [
      "android",
      "ios"
    ],
    "plugins": {
      "es6-promise-plugin": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "fluster",
        "ANDROID_SCHEME": " ",
        "ANDROID_HOST": " ",
        "ANDROID_PATHPREFIX": "/"
      },
      "cordova-plugin-file": {},
      "cordova-plugin-geolocation": {},
      "cordova-plugin-splashscreen": {},
